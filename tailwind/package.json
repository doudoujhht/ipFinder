{
  "name": "svelte-app",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "rollup -c",
    "serve": "nodemon src/app.js",
    "dev": "concurrently --kill-others \"npm run serve\"   \"npm run bus\" \"npm run start\"",
    "bus":"rollup -c -w",
    "start": "sirv public --no-clear --host"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-node-resolve": "^11.0.0",
    "rollup": "^2.3.4",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-svelte": "^7.0.0",
    "rollup-plugin-terser": "^7.0.0",
    "svelte": "^3.0.0"
  },
  "dependencies": {
    "axios": "^1.2.6",
    "concurrently": "^7.6.0",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "leaflet": "^1.9.3",
    "nodemon": "^2.0.20",
    "sirv-cli": "^2.0.0"
  }
}
